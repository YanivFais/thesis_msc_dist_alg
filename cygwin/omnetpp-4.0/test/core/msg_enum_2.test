%description:
Check "inheritance" with enums

%file: test.msg

namespace @TESTNAME@;

enum Base
{
    CSIGA = 10;
    BIGE = 12;
};

enum MyEnum extends Base
{
    ECC = 1;
    PECC = 2;
    KIMEHECC = 5;
};

%includes:
#include "test_m.h"

%activity:

#define PRINT(X) ev << #X << ":" << X << endl

PRINT(CSIGA);
PRINT(BIGE);

PRINT(ECC);
PRINT(PECC);
PRINT(KIMEHECC);

%contains: stdout
CSIGA:10
BIGE:12
ECC:1
PECC:2
KIMEHECC:5

