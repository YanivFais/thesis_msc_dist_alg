	-cd connectionManager && if [ -f Makefile ]; then $(MAKE) depend; fi
	-cd baseMobility && if [ -f Makefile ]; then $(MAKE) depend; fi
	-cd blackboard && if [ -f Makefile ]; then $(MAKE) depend; fi
	-cd testUtils && if [ -f Makefile ]; then $(MAKE) depend; fi

