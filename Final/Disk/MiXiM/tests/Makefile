.SUFFIXES: .cc

$O/%.o: %.cc
	@mkdir -p $(dir $@)
	$(CXX) -c $(COPTS) -o $@ $<

%_m.cc %_m.h: %.msg
	$(MSGC) -s _m.cc $(MSGCOPTS) $?

msgheaders: $(MSGFILES:.msg=_m.h)
	cd radioState && $(MAKE) msgheaders
	cd channelInfo && $(MAKE) msgheaders
	cd coord && $(MAKE) msgheaders
	cd basePhyLayer && $(MAKE) msgheaders
