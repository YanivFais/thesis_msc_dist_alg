#------------------------------------------------------------------------------

# Main target

all: $(OBJS) submakedirs Makefile
	@# Do nothing

submakedirs:  radioState_dir channelInfo_dir coord_dir basePhyLayer_dir mapping_dir connectionManager_dir baseMobility_dir blackboard_dir testUtils_dir

